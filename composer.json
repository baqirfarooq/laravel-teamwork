{
    "name": "baqirfarooq/laravel-teamwork",
    "description": "PHP wrapper for the Teamwork project management API",
    "keywords": ["teamwork", "api", "laravel", "project management"],
    "homepage": "https://github.com/baqirfarooq/laravel-teamwork",
    "license": "MIT",
    "authors": [
        {
            "name": "Baqir Farooq",
            "email": "bmconcepts@hotmail.com",
            "homepage": "https://github.com/baqirfarooq",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~5.5|~7.0",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "mockery/mockery": "0.9.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3",
        "orchestra/testbench": "~3.2"
    },
    "autoload": {
        "classmap": [
            "tests/FactoryTest.php",
            "tests/ClientTest.php",
            "tests/ObjectTest.php"
        ],
        "psr-4": {
            "Baqirfarooq\\Teamwork\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Baqirfarooq\\Teamwork\\": "tests"
        }
    },
    "suggest": {
        "illuminate/support": "Required for Laravel support"
    },
    "scripts": {
        "test": "phpunit"
    },

}
